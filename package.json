{
  "name": "divortio-logdo",
  "version": "1.0.0",
  "description": "Zero latency logging to Cloudflare D1 from Workers & Durable Objects",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply production-logs",
    "db:migrate:dry-run": "wrangler d1 migrations apply production-logs --local --dry-run"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240725.0",
    "wrangler": "^4.31.0"
  }
}